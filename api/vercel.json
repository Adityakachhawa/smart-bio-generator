{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-content.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generate-content",
      "dest": "api/generate-content.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ]
}